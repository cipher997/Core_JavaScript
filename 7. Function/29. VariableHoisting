    <script>
      // Variable Hoisting
      var i = "Hello";
      document.write(i + "<br>");
      function show() {
        document.write(i + "<br>");
        var i = "Kartik Gaikwad codes";
        document.write(i + "<br>");
      }
      show();

      /*
